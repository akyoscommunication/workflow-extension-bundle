{
	"name": "letots/workflow-extension-bundle",
	"type": "library",
	"description": "Configurations to allow defining Symfony Workflows as PHP classes easily and little helpers functions to work with them.",
	"keywords": ["workflow", "symfony", "PHP", "attribute"],
	"license": "MIT",
	"authors": [
		{
			"name": "Thomas Sebert",
			"email": "sebert.thomas19@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://symfony.com/contributors"
		}
	],
	"require": {
		"php": ">=8.3",
		"symfony/workflow": "^6.4|^7.0"
	},
	"require-dev": {
		"psr/log": "^1|^2|^3",
		"symfony/dependency-injection": "^6.4|^7.0",
		"symfony/event-dispatcher": "^6.4|^7.0",
		"symfony/error-handler": "^6.4|^7.0",
		"symfony/http-kernel": "^6.4|^7.0",
		"symfony/expression-language": "^6.4|^7.0",
		"symfony/stopwatch": "^6.4|^7.0",
		"symfony/validator": "^6.4|^7.0"
	},
	"autoload": {
		"psr-4": { "LeTots\\WorkflowExtension\\": "" },
		"exclude-from-classmap": [
			"/Tests/"
		]
	},
	"minimum-stability": "dev"
}